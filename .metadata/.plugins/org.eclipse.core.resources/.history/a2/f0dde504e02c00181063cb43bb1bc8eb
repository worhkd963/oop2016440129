import java.util.Scanner;

public class Eleven {

	public static void main(String[] args) {
		// 소수(prime-number) 검사와 흡사하게 사용자가 입력한 수의 인수들을 모두 출력하는 프로그램을 작성하시오.
		int a;
		int i=1;
		
		System.out.print("정수를 입력하세요 : ");
		a=new Scanner(System.in).nextInt();
		
		while(a!=0) {
			System.out.print(a+" => ");
			while(i<=a) {
				if(a%i==0)
					System.out.print(i+" ");
				i++;
			}
		}

	}

}
